<div class="row justify-content-center repair">
  <div class="col">
    <ul class="list-group list-group-flush list-group-horizontal green-icons">
      {% for icon in list_icons %}
        <li class="list-group-item">
          <img src="{{ icon.icon }}" />
          <em>{{ icon.title }}</em>
        </li>
      {% endfor %}
    </ul>
  </div>
</div>
